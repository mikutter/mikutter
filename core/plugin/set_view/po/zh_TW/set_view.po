msgstr "本文最大行數"

#: ../set_view.rb:95 ../set_view.rb:116
msgid "クリックされたときの挙動"
msgstr "點擊時的動作"

#: ../set_view.rb:96 ../set_view.rb:117
msgid "何もしない"
msgstr "什麼都不做"

#: ../set_view.rb:97 ../set_view.rb:118
